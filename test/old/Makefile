CPPOTHER 	=

CCDEBUG		= $(CC_OPTIMIZE)
CCOTHER		= $(CC_WARN)

CXXDEBUG	= $(CXX_OPTIMIZE)
CXXOTHER	= $(CXX_WARN)

# ##### I N S T A L L #####

INSTALL_BIN_FLAGS	= 

